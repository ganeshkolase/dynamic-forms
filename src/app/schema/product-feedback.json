{
  "title": "Product Feedback Form",
  "fields": [
    {
      "label": "Product Name",
      "name": "productName",
      "type": "text",
      "required": true,
      "minLength": 3
    },
    {
      "label": "Purchase Date",
      "name": "purchaseDate",
      "type": "date",
      "required": true
    },
    {
      "label": "Product Category",
      "name": "category",
      "type": "dropdown",
      "options": [
        "Electronics",
        "Clothing",
        "Home Appliances",
        "Books",
        "Other"
      ],
      "required": true
    },
    {
      "label": "Features you liked",
      "name": "likedFeatures",
      "type": "multiselect",
      "options": [
        "Design",
        "Durability",
        "Price",
        "Ease of Use",
        "Performance"
      ]
    },
    {
      "label": "Rating",
      "name": "rating",
      "type": "dropdown",
      "options": [
        "1 - Poor",
        "2 - Fair",
        "3 - Good",
        "4 - Very Good",
        "5 - Excellent"
      ],
      "required": true
    },
    {
      "label": "Would you recommend this product?",
      "name": "recommend",
      "type": "radiobutton",
      "options": [
        "Yes",
        "No"
      ],
      "required": true
    },
    {
      "label": "Additional Comments",
      "name": "comments",
      "type": "textarea",
      "maxLength": 250
    }
  ]
}
